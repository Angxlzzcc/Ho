Section:NewButton("Another bar Rainbow", "ButtonInfo", function()

local player = game:GetService("Players").LocalPlayer
local mainScreenGui = player.PlayerGui.MainScreenGui
local bar = mainScreenGui.Bar

local TweenService = game:GetService("TweenService")

-- ط¯ط§ظ„ط© ظ„طھط؛ظٹظٹط± ط§ظ„ط£ظ„ظˆط§ظ† ط¨ط´ظƒظ„ ظ…طھط¯ط±ط¬ ط¥ظ„ظ‰ ظ‚ظˆط³ ظ‚ط²ط­
local function updateRainbowColor(bar)
    local hue = 0
    while true do
        local color = Color3.fromHSV(hue, 1, 1)
        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
        local goal = {BackgroundColor3 = color}

        local tween = TweenService:Create(bar, tweenInfo, goal)
        tween:Play()

        hue = (hue + 0.01) % 1 -- طھط¹ط¯ظٹظ„ ظ‡ط°ظ‡ ط§ظ„ظ‚ظٹظ…ط© ظ„طھط؛ظٹظٹط± ط³ط±ط¹ط© ط§ظ„طھط£ط«ظٹط±
        wait(0.1) -- طھط¹ط¯ظٹظ„ ظ‡ط°ط§ ظ„طھط؛ظٹظٹط± ط³ط±ط¹ط© ط§ظ„طھط­ط¯ظٹط«
    end
end

-- ط¯ط§ظ„ط© ظ„طھط؛ظٹظٹط± ط£ظ„ظˆط§ظ† ط§ظ„ظ†طµظˆطµ ط¨ط´ظƒظ„ ظ…طھط¯ط±ط¬ ط¥ظ„ظ‰ ظ‚ظˆط³ ظ‚ط²ط­
local function updateRainbowTextColor(textLabel)
    local hue = 0
    while true do
        local color = Color3.fromHSV(hue, 1, 1)
        textLabel.TextColor3 = color
        hue = (hue + 0.01) % 1 -- طھط¹ط¯ظٹظ„ ظ‡ط°ظ‡ ط§ظ„ظ‚ظٹظ…ط© ظ„طھط؛ظٹظٹط± ط³ط±ط¹ط© ط§ظ„طھط£ط«ظٹط±
