local player = game:GetService("Players").LocalPlayer
local mainScreenGui = player.PlayerGui.MainScreenGui
local bar = mainScreenGui.Bar

local TweenService = game:GetService("TweenService")

-- Ø·Â¯Ø·Â§Ø¸â€Ø·Â© Ø¸â€Ø·Ú¾Ø·Ø›Ø¸Ù¹Ø¸Ù¹Ø·Â± Ø·Â§Ø¸â€Ø·Â£Ø¸â€Ø¸Ë†Ø·Â§Ø¸â€  Ø·Â¨Ø·Â´Ø¸Æ’Ø¸â€ Ø¸â€¦Ø·Ú¾Ø·Â¯Ø·Â±Ø·Â¬ Ø·Â¥Ø¸â€Ø¸â€° Ø¸â€šØ¸Ë†Ø·Â³ Ø¸â€šØ·Â²Ø·Â­
local function updateRainbowColor(bar)
    local hue = 0
    while true do
        local color = Color3.fromHSV(hue, 1, 1)
        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
        local goal = {BackgroundColor3 = color}

        local tween = TweenService:Create(bar, tweenInfo, goal)
        tween:Play()

        hue = (hue + 0.01) % 1 -- Ø·Ú¾Ø·Â¹Ø·Â¯Ø¸Ù¹Ø¸â€ Ø¸â€¡Ø·Â°Ø¸â€¡ Ø·Â§Ø¸â€Ø¸â€šØ¸Ù¹Ø¸â€¦Ø·Â© Ø¸â€Ø·Ú¾Ø·Ø›Ø¸Ù¹Ø¸Ù¹Ø·Â± Ø·Â³Ø·Â±Ø·Â¹Ø·Â© Ø·Â§Ø¸â€Ø·Ú¾Ø·Â£Ø·Â«Ø¸Ù¹Ø·Â±
        wait(0.1) -- Ø·Ú¾Ø·Â¹Ø·Â¯Ø¸Ù¹Ø¸â€ Ø¸â€¡Ø·Â°Ø·Â§ Ø¸â€Ø·Ú¾Ø·Ø›Ø¸Ù¹Ø¸Ù¹Ø·Â± Ø·Â³Ø·Â±Ø·Â¹Ø·Â© Ø·Â§Ø¸â€Ø·Ú¾Ø·Â­Ø·Â¯Ø¸Ù¹Ø·Â«
    end
end

-- Ø·Â¯Ø·Â§Ø¸â€Ø·Â© Ø¸â€Ø·Ú¾Ø·Ø›Ø¸Ù¹Ø¸Ù¹Ø·Â± Ø·Â£Ø¸â€Ø¸Ë†Ø·Â§Ø¸â€  Ø·Â§Ø¸â€Ø¸â€ Ø·ÂµØ¸Ë†Ø·Âµ Ø·Â¨Ø·Â´Ø¸Æ’Ø¸â€ Ø¸â€¦Ø·Ú¾Ø·Â¯Ø·Â±Ø·Â¬ Ø·Â¥Ø¸â€Ø¸â€° Ø¸â€šØ¸Ë†Ø·Â³ Ø¸â€šØ·Â²Ø·Â­
local function updateRainbowTextColor(textLabel)
    local hue = 0
    while true do
        local color = Color3.fromHSV(hue, 1, 1)
        textLabel.TextColor3 = color
        hue = (hue + 0.01) % 1 -- Ø·Ú¾Ø·Â¹Ø·Â¯Ø¸Ù¹Ø¸â€ Ø¸â€¡Ø·Â°Ø¸â€¡ Ø·Â§Ø¸â€Ø¸â€šØ¸Ù¹Ø¸â€¦Ø·Â© Ø¸â€Ø·Ú¾Ø·Ø›Ø¸Ù¹Ø¸Ù¹Ø·Â± Ø·Â³Ø·Â±Ø·Â¹Ø·Â© Ø·Â§Ø¸â€Ø·Ú¾Ø·Â£Ø·Â«Ø¸Ù¹Ø·Â±
        wait(0.1) -- Ø·Ú¾Ø·Â¹Ø·Â¯Ø¸Ù¹Ø¸â€ Ø¸â€¡Ø·Â°Ø·Â§ Ø¸â€Ø·Ú¾Ø·Ø›Ø¸Ù¹Ø¸Ù¹Ø·Â± Ø·Â³Ø·Â±Ø·Â¹Ø·Â© Ø·Â§Ø¸â€Ø·Ú¾Ø·Â­Ø·Â¯Ø¸Ù¹Ø·Â«
    end
end

-- Ø·Ú¾Ø·Â­Ø·Â¯Ø¸Ù¹Ø·Â¯ Ø·Â´Ø·Â±Ø¸Ù¹Ø·Â· Ø·Â§Ø¸â€Ø·ÂµØ·Â­Ø·Â©
local HPBar = mainScreenGui:FindFirstChild("HP", true)
if HPBar and HPBar:IsA("Frame") then
    spawn(function()
        updateRainbowColor(HPBar)
    end)
    local hpTextLabel = HPBar:FindFirstChild("TextLabel", true)
    if hpTextLabel then
        hpTextLabel.Text = "CRAZE.LOL"
        spawn(function()
            updateRainbowTextColor(hpTextLabel)
        end)
    end
    print("Changed Color Of HP Bar")
else
